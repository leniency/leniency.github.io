    <ul class="list-inline">
      <li class="list-inline-item">
        <a href="/">
          <Icon name="angle-left" />
          Home
        </a>
      </li>
      <li class="list-inline-item"></li>
    </ul>
  </header>
  <h2>{title}</h2>
  <Sphere client:only="react" />
</Layout>
